# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to source control

# Your Airtable personal access token
# Get it from: https://airtable.com/create/tokens
AIRTABLE_API_KEY=patXXXXXXXXXXXXXX

# Your Airtable Base ID (from the URL)
# Example: https://airtable.com/appXXXXXXXXX/tblYYYYYYYY
AIRTABLE_BASE_ID=appXXXXXXXXX

# Your Airtable Table Name
AIRTABLE_TABLE_NAME=People